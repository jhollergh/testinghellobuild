<?xml version="1.0" encoding="UTF-8"?>
<project name="module_pa1_starter" default="compile.module.pa1_starter">
  <dirname property="module.pa1_starter.basedir" file="${ant.file.module_pa1_starter}"/>
  
  <property name="module.jdk.home.pa1_starter" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.pa1_starter" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.pa1_starter" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.pa1_starter" value="-encoding UTF-8 -source 11 -target 11 ${compiler.args}"/>
  
  <property name="pa1_starter.output.dir" value="${module.pa1_starter.basedir}/out/production/PA1_Starter"/>
  <property name="pa1_starter.testoutput.dir" value="${module.pa1_starter.basedir}/out/test/PA1_Starter"/>
  
  <path id="pa1_starter.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="pa1_starter.module.production.classpath">
    <path refid="${module.jdk.classpath.pa1_starter}"/>
    <pathelement location="${path.variable.maven_repository}/org/apiguardian/apiguardian-api/1.0.0/apiguardian-api-1.0.0.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-api/5.4.2/junit-jupiter-api-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-engine/5.4.2/junit-jupiter-engine-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-params/5.4.2/junit-jupiter-params-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter/5.4.2/junit-jupiter-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/platform/junit-platform-commons/1.4.2/junit-platform-commons-1.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/platform/junit-platform-engine/1.4.2/junit-platform-engine-1.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/opentest4j/opentest4j/1.1.1/opentest4j-1.1.1.jar"/>
  </path>
  
  <path id="pa1_starter.runtime.production.module.classpath">
    <pathelement location="${pa1_starter.output.dir}"/>
    <pathelement location="${path.variable.maven_repository}/org/apiguardian/apiguardian-api/1.0.0/apiguardian-api-1.0.0.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-api/5.4.2/junit-jupiter-api-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-engine/5.4.2/junit-jupiter-engine-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-params/5.4.2/junit-jupiter-params-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter/5.4.2/junit-jupiter-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/platform/junit-platform-commons/1.4.2/junit-platform-commons-1.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/platform/junit-platform-engine/1.4.2/junit-platform-engine-1.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/opentest4j/opentest4j/1.1.1/opentest4j-1.1.1.jar"/>
  </path>
  
  <path id="pa1_starter.module.classpath">
    <path refid="${module.jdk.classpath.pa1_starter}"/>
    <pathelement location="${pa1_starter.output.dir}"/>
    <pathelement location="${path.variable.maven_repository}/org/apiguardian/apiguardian-api/1.0.0/apiguardian-api-1.0.0.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-api/5.4.2/junit-jupiter-api-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-engine/5.4.2/junit-jupiter-engine-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-params/5.4.2/junit-jupiter-params-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter/5.4.2/junit-jupiter-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/platform/junit-platform-commons/1.4.2/junit-platform-commons-1.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/platform/junit-platform-engine/1.4.2/junit-platform-engine-1.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/opentest4j/opentest4j/1.1.1/opentest4j-1.1.1.jar"/>
  </path>
  
  <path id="pa1_starter.runtime.module.classpath">
    <pathelement location="${pa1_starter.testoutput.dir}"/>
    <pathelement location="${pa1_starter.output.dir}"/>
    <pathelement location="${path.variable.maven_repository}/org/apiguardian/apiguardian-api/1.0.0/apiguardian-api-1.0.0.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-api/5.4.2/junit-jupiter-api-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-engine/5.4.2/junit-jupiter-engine-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter-params/5.4.2/junit-jupiter-params-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/jupiter/junit-jupiter/5.4.2/junit-jupiter-5.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/platform/junit-platform-commons/1.4.2/junit-platform-commons-1.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/junit/platform/junit-platform-engine/1.4.2/junit-platform-engine-1.4.2.jar"/>
    <pathelement location="${path.variable.maven_repository}/org/opentest4j/opentest4j/1.1.1/opentest4j-1.1.1.jar"/>
  </path>
  
  
  <patternset id="excluded.from.module.pa1_starter">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.pa1_starter">
    <patternset refid="excluded.from.module.pa1_starter"/>
  </patternset>
  
  <path id="pa1_starter.module.sourcepath">
    <dirset dir="${module.pa1_starter.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.pa1_starter" depends="compile.module.pa1_starter.production,compile.module.pa1_starter.tests" description="Compile module PA1_Starter"/>
  
  <target name="compile.module.pa1_starter.production" depends="register.custom.compilers" description="Compile module PA1_Starter; production classes">
    <mkdir dir="${pa1_starter.output.dir}"/>
    <javac2 destdir="${pa1_starter.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.pa1_starter}/javac">
      <compilerarg line="${compiler.args.pa1_starter}"/>
      <bootclasspath refid="pa1_starter.module.bootclasspath"/>
      <classpath refid="pa1_starter.module.production.classpath"/>
      <src refid="pa1_starter.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.pa1_starter"/>
    </javac2>
    
    <copy todir="${pa1_starter.output.dir}">
      <fileset dir="${module.pa1_starter.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.pa1_starter.tests" depends="register.custom.compilers,compile.module.pa1_starter.production" description="compile module PA1_Starter; test classes" unless="skip.tests"/>
  
  <target name="clean.module.pa1_starter" description="cleanup module">
    <delete dir="${pa1_starter.output.dir}"/>
    <delete dir="${pa1_starter.testoutput.dir}"/>
  </target>
</project>